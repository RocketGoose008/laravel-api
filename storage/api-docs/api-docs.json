        "/api/transactions/insert": {
            "post": {
                "tags": [
                    "Transactions"
                ],
                "summary": "Insert new transaction (mockup api)",
                "operationId": "eea7d27d831e444fb9dd687136ad1119",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "transaction_type",
                                    "token_type",
                                    "status",
                                    "payment_type",
                                    "system_type",
                                    "pay_currency",
                                    "amount",
                                    "per_price",
                                    "total_price",
                                    "sender_id",
                                    "receiver_id",
                                    "sell_order_id"
                                ],
                                "properties": {
                                    "transaction_type": {
                                        "description": "value: buy,sell,transfer,receive",
                                        "type": "string",
                                        "example": "buy"
                                    },
                                    "token_type": {
                                        "description": "value: BTC,ETH,XRP,DOGE",
                                        "type": "string",
                                        "example": "XRP"
                                    },
                                    "status": {
                                        "description": "value: pending,processing,completed,failed",
                                        "type": "string",
                                        "example": "pending"
                                    },
                                    "payment_type": {
                                        "description": "value: qrcode,credit",
                                        "type": "string",
                                        "example": "credit"
                                    },
                                    "system_type": {
                                        "description": "value: internal,external",
                                        "type": "string",
                                        "example": "internal"
                                    },
                                    "pay_currency": {
                                        "type": "string",
                                        "example": "USD"
                                    },
                                    "amount": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "per_price": {
                                        "type": "decimal",
                                        "example": 500
                                    },
                                    "total_price": {
                                        "type": "number",
                                        "example": 1000
                                    },
                                    "sender_id": {
                                        "type": "string",
                                        "example": "abc527bd-79d0-4799-a883-9c7257990d3a"
                                    },
                                    "receiver_id": {
                                        "type": "string",
                                        "example": "3535a148-2431-48ea-a1bf-314e52a8fb56"
                                    },
                                    "sell_order_id": {
                                        "type": "string",
                                        "example": "5ca2d56d-79ad-41e3-889f-0cdc12c77014"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "code": {
                                            "type": "integer",
                                            "example": 201
                                        },
                                        "msg": {
                                            "type": "string",
                                            "example": "Success"
                                        },
                                        "data": {
                                            "properties": {
                                                "id": {
                                                    "type": "string",
                                                    "example": "9390cea0-6fa6-41f3-a003-b5c3fb69db3a"
                                                },
                                                "create_date": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2025-05-16 11:35:59"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Insert transaction failed"
                    }
                }
            }
        },
